{
    "name": "spacetab-io/harmony-cache",
    "description": "Asynchronous caching library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Pieter Hordijk",
            "email": "info@pieterhordijk.com"
        }
    ],
    "prefer-stable": true,
    "require": {
        "php": ">=7.3",
        "amphp/amp": "^2.4",
        "amphp/cache": "^1.3",
        "amphp/redis": "^1.0",
        "amphp/uri": "^0.1"
    },
    "require-dev": {
        "amphp/phpunit-util": "^1.3",
        "harmonyio/php-codesniffer-ruleset": "dev-master",
        "maglnet/composer-require-checker": "^2.0.0",
        "phpunit/phpunit": "^9",
        "slevomat/coding-standard": "^5.0.4",
        "squizlabs/php_codesniffer": "^3.4.2"
    },
    "autoload": {
        "psr-4": {
            "HarmonyIO\\Cache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HarmonyIO\\CacheTest\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
